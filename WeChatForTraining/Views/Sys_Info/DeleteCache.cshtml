@model Lythen.ViewModel.DeleteCacheModel
@{
    ViewBag.Title = "缓存管理";
    Layout = "~/Views/Shared/managerLayout.cshtml";
}
<style>
    .cache-ul li{float:left;list-style:none;width:180px;height:30px;line-height:30px;text-align:left;}
</style>
<h4>缓存管理</h4>
<hr />
@using (Html.BeginForm("DeleteCache", "Sys_Info", FormMethod.Post))
{
    @Html.AntiForgeryToken()
<ul class="cache-ul">
    <li>

        @Html.CheckBoxFor(model => model.cache_subjects)
        @Html.LabelFor(model => model.cache_subjects, new { @for = "cache_subjects" })
    </li>
    <li>

        @Html.CheckBoxFor(model => model.cache_teachers)
        @Html.LabelFor(model => model.cache_teachers, new { @for = "cache_teachers" })
    </li>
    <li>

        @Html.CheckBoxFor(model => model.cache_assistants)
        @Html.LabelFor(model => model.cache_assistants, new { @for = "cache_assistants" })
    </li>
    <li>

        @Html.CheckBoxFor(model => model.cache_sysschools)
        @Html.LabelFor(model => model.cache_sysschools, new { @for = "cache_sysschools" })
    </li>
    <li>

        @Html.CheckBoxFor(model => model.cache_seasons)
        @Html.LabelFor(model => model.cache_seasons, new { @for = "cache_seasons" })
    </li>
    <li>

        @Html.CheckBoxFor(model => model.cache_school)
        @Html.LabelFor(model => model.cache_school, new { @for = "cache_school" })
    </li>
    <li>

        @Html.CheckBoxFor(model => model.cache_course)
        @Html.LabelFor(model => model.cache_course, new { @for = "cache_course" })
    </li>
    <li>

        @Html.CheckBoxFor(model => model.cache_room)
        @Html.LabelFor(model => model.cache_room, new { @for = "cache_room" })
    </li>
    <li>

        @Html.CheckBoxFor(model => model.cache_roles)
        @Html.LabelFor(model => model.cache_roles, new { @for = "cache_roles" })
    </li>
    <li>

        @Html.CheckBoxFor(model => model.cache_managers)
        @Html.LabelFor(model => model.cache_managers, new { @for = "cache_managers" })
    </li>
    <li>

        @Html.CheckBoxFor(model => model.cache_managerTeachers)
        @Html.LabelFor(model => model.cache_managerTeachers, new { @for = "cache_managerTeachers" })
    </li>
</ul>
<div class="form-group">
    <div class="col-md-10" style="padding-left:40px;">
        <input type="submit" value="清除选定" class="btn btn-default" />
    </div>
</div>
<div class="form-group">
    <div class="col-md-10 log-msg" style="color:red;padding-left:40px;">@ViewBag.msg</div>
</div>
}
<script src="~/scripts/jquery-1.10.2.min.js"></script>
<script>
    $(function () {
        if ($('.log-msg').text().length>0) {
            setTimeout(function () {
                $('.log-msg').slideToggle();
            }, 3000);
        }
    });
</script>